{
	"arrowParens": "always",
	"semi": true,
	"useTabs": true,
	"tabWidth": 2,
	"printWidth": 80,
	"singleQuote": true,
	"jsxSingleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
