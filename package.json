		"astro": "^3.5.4",
		"clsx": "^2.0.0",
		"fuse.js": "^7.0.0",
		"github-slugger": "^2.0.0",
		"react-icons": "^4.12.0",
		"remark-collapse": "^0.1.2",
		"remark-toc": "^9.0.0",
		"satori": "^0.10.9",
		"tailwindcss": "^3.3.5"
	},
	"devDependencies": {
		"@astrojs/react": "^3.0.4",
		"@astrojs/sitemap": "^3.0.3",
		"@astrojs/tailwind": "^5.0.2",
		"@divriots/jampack": "^0.22.1",
		"@eslint/eslintrc": "^2.1.3",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "^8.44.7",
		"@types/github-slugger": "^1.3.0",
